{
  "$kind": "Microsoft.AdaptiveDialog",
  "$designer": {
    "id": "XXdQ0A",
    "name": "call_openai",
    "comment": "Does a code review"
  },
  "autoEndDialog": true,
  "defaultResultProperty": "dialog.result",
  "triggers": [
    {
      "$kind": "Microsoft.OnBeginDialog",
      "$designer": {
        "name": "BeginDialog",
        "description": "",
        "id": "ak3CwT"
      },
      "actions": [
        {
          "$kind": "Microsoft.HttpRequest",
          "$designer": {
            "id": "GDur5y"
          },
          "resultProperty": "dialog.api_response",
          "url": " https://api.openai.com/v1/completions",
          "method": "POST",
          "headers": {
            "Authorization": "Bearer ${settings.OPENAI_API_KEY}"
          },
          "contentType": "application/json",
          "responseType": "json",
          "body": {
            "model": "text-davinci-003",
            "prompt": "${dialog.prompt} \n\n${dialog.code}\"\"\"",
            "max_tokens": 512,
            "temperature": 0,
            "top_p": 1,
            "frequency_penalty": 0,
            "presence_penalty": 0,
            "stop": [
              "\"\"\""
            ]
          }
        },
        {
          "$kind": "Microsoft.IfCondition",
          "$designer": {
            "id": "Cwvjap"
          },
          "condition": "=dialog.api_response.statusCode == 200",
          "actions": [
            {
              "$kind": "Microsoft.SetProperties",
              "$designer": {
                "id": "crMgOX"
              },
              "assignments": [
                {
                  "property": "dialog.result",
                  "value": "=dialog.api_response.content.choices[0].text"
                }
              ]
            },
            {
              "$kind": "Microsoft.SendActivity",
              "$designer": {
                "id": "pSBPiy"
              },
              "activity": "${SendActivity_pSBPiy()}"
            }
          ],
          "elseActions": [
            {
              "$kind": "Microsoft.SendActivity",
              "$designer": {
                "id": "TmbAVO"
              },
              "activity": "${SendActivity_TmbAVO()}"
            }
          ]
        }
      ]
    }
  ],
  "generator": "do_code_review.lg",
  "recognizer": "do_code_review.lu.qna",
  "id": "do_code_review"
}
